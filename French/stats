echo "--------------------------------"
printf "no score: "
egrep 999 .french_vocab.voc -c
printf "score=12: "
grep 12 .french_vocab.voc -c
printf "score=13: "
grep 13 .french_vocab.voc -c
printf "score=14: "
grep 14 .french_vocab.voc -c
printf "score=15: "
grep 15 .french_vocab.voc -c
echo "--------------------------------"

